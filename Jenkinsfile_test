pipeline {
    agent any
    stages {
        stage('Package Builds') {
            parallel {
                stage('script bool compare true') {
                    when {
                        expression {
                            return sh(returnStdout: true, script: './is-package-affected.sh e2e-tests') == true
                        }
                    }
                    stages {
                        stage('script bool compare true') {
                            steps {
                                echo "script bool compare true"
                            }
                        }
                    }
                }
                stage('script bool compare false') {
                    when {
                        expression {
                            return sh(returnStdout: true, script: './is-package-affected.sh frontend') == true
                        }
                    }
                    stages {
                        stage('script bool compare false') {
                            steps {
                                echo "script bool compare false"
                            }
                        }
                    }
                }
            }
        }
    }
}
